{
  "name": "skiff-icons",
  "version": "1.0.0",
  "description": "A comprehensive icon library for React applications with 300+ icons",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./icons": {
      "types": "./dist/icons.d.ts",
      "import": "./dist/icons.mjs",
      "require": "./dist/icons.js"
    }
  },
  "files": [
    "dist",
    "Icons",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "icons",
    "react",
    "svg",
    "ui",
    "components",
    "typescript",
    "design-system",
    "icon-library"
  ],
  "author": "Skiff",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/skiff-org/skiff-icons.git"
  },
  "bugs": {
    "url": "https://github.com/skiff-org/skiff-icons/issues"
  },
  "homepage": "https://github.com/skiff-org/skiff-icons#readme",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "sideEffects": false
}
